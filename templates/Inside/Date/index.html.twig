{# templates/Inside/Date/index.html.twig #}

{% extends "Inside/layout.html.twig" %}

{% block title %}
    {{ parent() }} - Date
{% endblock %}


{% block quote_body %}





    <table class="table table-hover">
        {% for date in dates %}
            {% for d in date %}
                {% if d is not empty %}
                    <tr>
                        <td>{{ d }}</td>
                        <td><a href="{{ path('qtf_work_year', {'year': d}) }}">xx quotes</a></td>
                    </tr>
                {% else %}
                    <tr>
                        <td>Undefined</td>
                        <td><a href="{{ path('qtf_work_year', {'year': 9999}) }}">xx quotes</a></td>
                    </tr>
                {% endif %}
            {% endfor %}
        {% endfor %}
    </table>






{% endblock %}